  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:3000",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}",
      "sourceMapPathOverrides": {
          "webpack:///*": "*",
          "webpack:///./*": "${webRoot}/*",
          "webpack:///src/client/*": "${webRoot}/*",
          "webpack:///./~/*": "${webRoot}/node_modules/*"
      },
      "trace": true
    },
